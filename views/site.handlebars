<section class="container">
  <h3 class="site-name">{{park.name}}</h3>
  <h4>Season Opening  Hour: {{format_time site.starting_date}}</h4>
  <h4>Season Closing Hour: {{format_time site.ending_date}}</h4>
  <section class="flex-row justify-center">
    {{#each site.cherrys as |cherry|}}
    {{> cherry-details}}
    {{/each}}
  </section>
</section>
